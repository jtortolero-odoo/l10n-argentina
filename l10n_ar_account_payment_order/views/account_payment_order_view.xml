<?xml version="1.0" encoding="UTF-8"?>

<odoo>
    <data>

        <!-- Customer Views, Actions, and menus -->
        <!-- Receipts -->

        <record id="account_payment_order_view_filter_receipt" model="ir.ui.view">
            <field name="name">account.payment.order.customer.pay.select</field>
            <field name="model">account.payment.order</field>
            <field name="arch" type="xml">
                <search string="Search Vouchers">
                    <field name="number" string="Voucher" />
                    <field name="date" />
                    <filter name="draft" icon="terp-document-new" string="Draft" domain="[('state','=','draft')]" help="Draft Vouchers" />
                    <filter name="posted" icon="terp-camera_test" string="Posted" domain="[('state','=','posted')]" help="Posted Vouchers" />
                    <field name="partner_id" string="Customer" filter_domain="[('partner_id','child_of',self)]" />
                    <field name="journal_id" widget="selection" context="{'journal_id': self, 'set_visible':False}" domain="[('type','=','receipt')]" />
                    <group expand="0" string="Group By">
                        <filter name="customer" string="Customer" icon="terp-personal" domain="[]" context="{'group_by':'partner_id'}" />
                        <filter name="journal" string="Journal" icon="terp-folder-orange" domain="[]" context="{'group_by':'journal_id'}" />
                        <filter name="period" string="Period" icon="terp-go-month" domain="[]" context="{'group_by':'period_id','visible':True}" />
                        <filter name="status" string="Status" icon="terp-stock_effects-object-colorize" domain="[]" context="{'group_by':'state'}" />
                    </group>
                </search>
            </field>
        </record>

        <record id="account_payment_order_view_tree_receipt" model="ir.ui.view">
            <field name="name">account.payment.order.receipt.tree</field>
            <field name="model">account.payment.order</field>
            <field name="arch" type="xml">
                <tree string="Payment Order" name="payment_order_tree" decoration-info="state == 'draft'" decoration-muted="state  == 'cancel'">
                    <field name="date" />
                    <field name="number" />
                    <field name="name" />
                    <field name="partner_id" />
                    <field name="currency_id" />
                    <field name="amount" />
                    <field name="state" />
                </tree>
            </field>
        </record>


        <record id="account_payment_order_view_form_receipt" model="ir.ui.view">
            <field name="name">account.payment.order.receipt.form</field>
            <field name="model">account.payment.order</field>
            <field name="arch" type="xml">
                <form string="Receipt">
                    <header invisible="context.get('line_type', False)">
                        <button name="proforma_voucher" string="Validate" class="oe_highlight" type="object" states="draft" />
                        <button name="cancel_voucher" string="Cancel Receipt" type="object" states="draft,proforma" />
                        <button name="cancel_voucher" string="Unreconcile" type="object" states="posted" confirm="Are you sure to unreconcile and cancel this record ?" />
                        <button name="action_cancel_draft" type="object" states="cancel" string="Set to Draft" />
                        <field name="state" widget="statusbar" statusbar_visible="draft,posted" type="object" statusbar_colors='{"proforma":"blue"}' />
                    </header>
                    <sheet>
                        <h1 attrs="{'invisible': [('number','=',False)]}">
                            <field name="number" attrs="{'readonly': [('state', 'not in', ('draft'))]}" />
                        </h1>
                        <group invisible="context.get('line_type', False)">
                            <group>
                                <field name="partner_id" domain="[('customer','=',True)]" required="1" string="Customer" context="{'search_default_customer': 1}" attrs="{'readonly': [('state', 'not in', ('draft'))]}" />
                                <field name="currency_id" invisible="1" />
                                <field name="journal_id" domain="[('type','=','receipt'), ('company_id', '=', company_id)]" string="Journal" options="{'no_quick_create': True, 'no_create': True, 'no_create_edit': True}" attrs="{'readonly': [('state', 'not in', ('draft'))]}" />
                                <field name="amount" class="oe_inline" string="Paid Amount" widget="monetary" options="{'currency_field': 'currency_id'}" force_save="True" attrs="{'readonly': True}" />
                            </group>
                            <group>
                                <field name="date" attrs="{'readonly': [('state', 'not in', ('draft'))]}" />
                                <field name="period_id" />
                                <field name="reference" string="Payment Ref" placeholder="e.g. 003/10" attrs="{'readonly': [('state', 'not in', ('draft'))]}" />
                                <field name="name" colspan="2" placeholder="e.g. Invoice SAJ/0042" attrs="{'readonly': [('state', 'not in', ('draft'))]}" />
                                <field name="company_id" readonly="1" groups="base.group_multi_company" />
                                <field name="account_id" widget="selection" invisible="True" attrs="{'readonly': [('state', 'not in', ('draft'))]}" />
                                <field name="pre_line" invisible="1" />
                                <field name="type" invisible="1" attrs="{'readonly': [('state', 'not in', ('draft'))]}" />
                                <field name="currency_id" attrs="{'readonly': True}" force_save="True" />
                            </group>
                            <group col="4" attrs="{'invisible':[('is_multi_currency','=',False)]}">
                                <field name="is_multi_currency" invisible="1" />
                                <label for="payment_rate" colspan="1" />
                                <div>
                                    <field name="payment_rate" required="1" class="oe_inline" attrs="{'readonly': [('state', 'not in', ('draft'))]}" />
                                    <field name="payment_rate_currency_id" class="oe_inline" attrs="{'readonly': [('state', 'not in', ('draft'))]}" groups="base.group_multi_currency" />
                                </div>
                                <!-- <field name="currency_help_label" colspan="2" nolabel="1" class="oe_grey"/> -->
                                <field name="paid_amount_in_company_currency" colspan="4" invisible="1" />
                            </group>
                            <group col="2">
                                <separator string="Payment Options" colspan="2" />
                                <field name="writeoff_amount" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                <field name="payment_option" required="1" attrs="{'invisible':[('writeoff_amount','=',0)], 'readonly': [('state', 'not in', ('draft'))]}" />
                                <field name="writeoff_acc_id" attrs="{'invisible':['|', ('payment_option','!=','with_writeoff'), ('writeoff_amount','=',0)], 'required':[('payment_option','=','with_writeoff')], 'readonly': [('state', 'not in', ('draft'))]}" domain="[('type','=','other')]" />
                                <field name="comment" attrs="{'invisible':['|', ('payment_option','!=','with_writeoff'), ('writeoff_amount','=',0)], 'readonly': [('state', 'not in', ('draft'))]}" />
                                <field name="analytic_id" groups="analytic.group_analytic_accounting" attrs="{'invisible':['|', ('payment_option','!=','with_writeoff'), ('writeoff_amount','=',0)], 'readonly': [('state', 'not in', ('draft'))]}" />
                            </group>
                        </group>
                        <notebook>
                            <page string="Payment Information" name="payment_information" groups="base.group_user">
                                <label for="payment_mode_line_ids" />
                                <field name="payment_mode_line_ids" attrs="{'readonly': [('state', 'not in', ('draft'))]}">
                                    <tree string="Payment Methods" editable="bottom" decoration-muted="amount == 0">
                                        <field name="payment_mode_id" invisible="0" attrs="{'required': True}" domain="[('type', 'in', ['cash', 'bank']), ('company_id', '=', parent.company_id)]" />
                                        <field name="name" invisible="1" />
                                        <field name="currency_id" readonly="1" />
                                        <field name="date" />
                                        <field name="payment_order_id" invisible="1" />
                                        <field name="name" invisible="0" />
                                        <field name="amount" sum="Total" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                    </tree>
                                </field>
                                <div class="oe_button_box" name="button_box">
                                    <button name="gather_income_lines" attrs="{'invisible': [('state', 'in', ['cancel', 'posted'])]}" type="object" class="oe_stat_button" icon="fa-refresh slow-spin">
                                        Gather Lines
                                    </button>
                                </div>
                                <field name="invoice_ids" widget="many2many_tags" invisible="1" />
                                <label for="income_line_ids" invisible="context.get('line_type', False)" string="Invoices and outstanding transactions" />
                                <field name="income_line_ids" context="{'journal_id':journal_id, 'type':type, 'partner_id':partner_id}" attrs="{'readonly': [('state', 'not in', ('draft'))]}">
                                    <tree string="Invoices and outstanding transactions" editable="bottom" decoration-muted="amount == 0" decoration-success="amount == amount_unreconciled">
                                        <field name="move_invoice_id" invisible="1" />
                                        <field name="state" invisible="1" />
                                        <field name="invoice_id" force_save="true" domain="[('state', '=', 'open'),
                                            ('commercial_partner_id', '=', parent.partner_id),
                                            ('id', 'not in', parent.invoice_ids),
                                            ('type', '=', 'out_invoice')]" attrs="{
                                            'required': ['|',
                                            ('move_line_id', '=', False),
                                            ('move_invoice_id', '!=', False)],
                                            'readonly': [('move_line_id', '!=', False)]}" options="{'no_quick_create': True, 'no_create_edit': True}" />
                                        <field name="ref" readonly="1" force_save="True" />
                                        <field name="move_line_id" context="{'journal_id':parent.journal_id, 'partner_id':parent.partner_id}" domain="[('account_id.internal_type','in',('receivable','payable')), ('reconciled','=', False), ('partner_id','=',parent.partner_id)]" readonly="True" groups="account.group_account_user" force_save="True" />
                                        <field name="account_id" groups="base.group_no_one" domain="[('internal_type','=','receivable')]" readonly="True" force_save="True" />
                                        <field name="date_original" readonly="1" force_save="True" />
                                        <field name="date_due" readonly="1" force_save="True" />
                                        <field name="currency_id" invisible="1" />
                                        <field name="amount_original" readonly="1" force_save="True" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                        <field name="amount_unreconciled" readonly="1" groups="account.group_account_user" widget="monetary" options="{'currency_field': 'currency_id'}" sum="Total Unreconciled" force_save="True" />
                                        <field name="amount" sum="Total Allocation" string="Allocation" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                        <field string="Full" name="reconcile" class="reconcile_bullseye" />
                                    </tree>
                                </field>
                                <div class="oe_button_box" name="button_box">
                                    <button name="gather_debt_lines" attrs="{'invisible': [('state', 'in', ['cancel', 'posted'])]}" type="object" class="oe_stat_button" icon="fa-refresh slow-spin">
                                        Gather Lines
                                    </button>
                                </div>
                                <label for="debt_line_ids" string="Credits" />
                                <field name="debt_line_ids" attrs="{'readonly': [('state', 'not in', ('draft'))]}" context="{'journal_id':journal_id, 'partner_id':partner_id}">
                                    <tree string="Credits" editable="bottom" decoration-muted="amount == 0" decoration-success="amount == amount_unreconciled">
                                        <field name="move_invoice_id" invisible="1" />
                                        <field name="state" invisible="1" />
                                        <field name="invoice_id" force_save="true" domain="[('state', '=', 'open'),
                                            ('commercial_partner_id', '=', parent.partner_id),
                                            ('id', 'not in', parent.invoice_ids),
                                            ('type', '=', 'out_refund')]" attrs="{
                                            'required': ['|',
                                            ('move_line_id', '=', False),
                                            ('move_invoice_id', '!=', False)],
                                            'readonly': [('move_line_id', '!=', False)]}" options="{'no_quick_create': True, 'no_create_edit': True}" />
                                        <field name="ref" attrs="{'readonly': True}" force_save="True" />
                                        <field name="move_line_id" context="{'journal_id':parent.journal_id, 'partner_id':parent.partner_id}" domain="[('account_id.internal_type','in',('receivable','payable')), ('reconciled','=', False), ('partner_id','=',parent.partner_id)]" attrs="{'readonly': True}" force_save="True" />
                                        <field name="account_id" groups="base.group_no_one" domain="[('internal_type','=','receivable')]" readonly="1" force_save="True" />
                                        <field name="date_original" readonly="1" force_save="True" />
                                        <field name="date_due" readonly="1" force_save="True" />
                                        <field name="currency_id" invisible="1" />
                                        <field name="amount_original" readonly="1" force_save="True" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                        <field name="amount_unreconciled" readonly="1" sum="Total Unreconciled" force_save="True" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                        <field name="reconcile" readonly="1" force_save="True" invisible="1" />
                                        <field name="amount" sum="Total Allocation" string="Allocation" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                        <field string="Full" name="reconcile" class="reconcile_bullseye" />
                                    </tree>
                                </field>
                                <group col="3">
                                    <group>
                                        <field name="narration" colspan="2" nolabel="1" attrs="{'readonly': [('state', 'not in', ('draft'))]}" />
                                    </group>
                                </group>
                            </page>
                            <page string="Journal Items" name="journal_items" attrs="{'invisible': [('state','!=','posted')]}">
                                <group col="4">
                                    <!-- TODO: Este campo era un related a 'to_check' en account.move pero ya no existe mas -->
                                    <!-- <field name="audit"/> -->
                                    <field name="move_id" />
                                </group>
                                <field name="move_line_ids" readonly="1">
                                    <tree string="Journal Items">
                                        <field name="move_id" />
                                        <field name="ref" />
                                        <field name="date" />
                                        <field name="statement_id" />
                                        <field name="partner_id" />
                                        <field name="account_id" />
                                        <field name="name" />
                                        <field name="debit" />
                                        <field name="credit" />
                                        <!-- TODO: No existe mas el campo state -->
                                        <!-- <field name="state"/> -->
                                        <!-- TODO: No existe el modelo 'account.move.reconcile' -->
                                        <!-- <field name="reconcile_id"/> -->
                                        <field name="amount_currency" />
                                        <field name="currency_id" groups="base.group_multi_currency" />
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter" invisible="context.get('line_type', False)">
                        <field name="message_follower_ids" widget="mail_followers" />
                        <field name="message_ids" widget="mail_thread" />
                    </div>
                </form>
            </field>
        </record>


        <record id="account_payment_order_action_receipt" model="ir.actions.act_window">
            <field name="name">Customer Payments</field>
            <field name="res_model">account.payment.order</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('journal_id.type', '=', 'receipt'), ('type','=','receipt')]</field>
            <field name="context">{'type':'receipt'}</field>
            <field name="view_id" eval="False" />
            <field name="target">current</field>
            <field name="search_view_id" ref="account_payment_order_view_filter_receipt" />
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to register a new payment.
                    </p>
                <p>
                    Enter the customer and the payment method and then, either
                    create manually a payment record or Odoo will propose to you
                    automatically the reconciliation of this payment with the open
                    invoices or sales receipts.
                </p>
            </field>
        </record>

        <record id="account_payment_order_action_receipt_tree" model="ir.actions.act_window.view">
            <field eval="1" name="sequence" />
            <field name="view_mode">tree</field>
            <field name="act_window_id" ref="account_payment_order_action_receipt" />
        </record>
        <record id="account_payment_order_action_receipt_form" model="ir.actions.act_window.view">
            <field eval="2" name="sequence" />
            <field name="view_mode">form</field>
            <field name="view_id" ref="account_payment_order_view_form_receipt" />
            <field name="act_window_id" ref="account_payment_order_action_receipt" />
        </record>

        <menuitem id="account_payment_order_receipt_menu" action="account_payment_order_action_receipt" parent="account.menu_finance_receivables" sequence="10" />


        <!-- ###################################################################### -->


        <!-- Vendor Views, Actions, and menus -->
        <!-- Payments -->
        <record id="account_payment_order_view_filter_vendor_pay" model="ir.ui.view">
            <field name="name">account.payment.order.purchase.pay.select</field>
            <field name="model">account.payment.order</field>
            <field name="arch" type="xml">
                <search string="Search Vouchers">
                    <field name="number" string="Voucher" />
                    <field name="date" />
                    <filter name="draft" icon="terp-document-new" string="Draft" domain="[('state','=','draft')]" help="Draft Vouchers" />
                    <filter name="posted" icon="terp-camera_test" string="Posted" domain="[('state','=','posted')]" help="Posted Vouchers" />
                    <field name="partner_id" string="Supplier" filter_domain="[('partner_id','child_of',self)]" />
                    <field name="journal_id" widget="selection" context="{'journal_id': self, 'set_visible':False}" domain="[('type','in',('bank',    'cash'))]" /> <!-- Keep widget=selection on this field to pass numeric `self` value, which is not the case for regular m2o widgets! -->
                    <!-- <field name="period_id"/> -->
                    <group expand="0" string="Group By">
                        <filter name="supplier" string="Supplier" icon="terp-personal" domain="[]" context="{'group_by':'partner_id'}" />
                        <filter name="journal" string="Journal" icon="terp-folder-orange" domain="[]" context="{'group_by':'journal_id'}" />
                        <filter name="period" string="Period" icon="terp-go-month" domain="[]" context="{'group_by':'period_id','visible':True}" />
                        <filter name="status" string="Status" icon="terp-stock_effects-object-colorize" domain="[]" context="{'group_by':'state'}" />
                    </group>
                </search>
            </field>
        </record>

        <!-- Supplier Payment -->
        <record id="account_payment_order_view_form_vendor" model="ir.ui.view">
            <field name="name">account.payment.order.form</field>
            <field name="model">account.payment.order</field>
            <field name="arch" type="xml">
                <form string="Payment Order">
                    <header>
                        <button name="proforma_voucher" string="Validate" states="draft" invisible="context.get('line_type', False)" class="oe_highlight" type="object" />
                        <button name="cancel_voucher" string="Cancel Payment" states="draft,proforma" type="object" invisible="context.get('line_type', False)" />
                        <button name="cancel_voucher" string="Unreconcile" type="object" states="posted" invisible="context.get('line_type', False)" confirm="Are you sure to unreconcile and cancel this record ?" />
                        <button name="action_cancel_draft" type="object" states="cancel" string="Set to Draft" invisible="context.get('line_type', False)" />
                        <field name="state" widget="statusbar" statusbar_visible="draft,posted" statusbar_colors='{"proforma":"blue"}' />
                    </header>
                    <sheet>
                        <h1 attrs="{'invisible': [('number','=',False)]}">
                            <field name="number" />
                        </h1>
                        <field name="account_id" invisible="True" />
                        <field name="pre_line" invisible="1" />
                        <field name="type" invisible="True" />
                        <group>
                            <group>
                                <field name="partner_id" required="1" invisible="context.get('line_type', False)" context="{'invoice_currency':currency_id, 'default_customer': 0, 'search_default_supplier': 1, 'default_supplier': 1}" domain="[('supplier','=',True)]" string="Supplier" />
                                <field name="currency_id" invisible="1" />
                                <field name="journal_id" domain="[('type', '=', 'payment'), ('company_id', '=', company_id)]" invisible="context.get('line_type', False)" options="{'no_quick_create': True, 'no_create': True, 'no_create_edit': True}" string="Journal" />
                                <field name="amount" class="oe_inline" string="Paid Amount" widget="monetary" options="{'currency_field': 'currency_id'}" force_save="True" attrs="{'readonly': True}" />
                            </group>
                            <group>
                                <field name="date" invisible="context.get('line_type', False)" />
                                <field name="period_id" />
                                <field name="reference" invisible="context.get('line_type', False)" string="Payment Ref" placeholder="e.g. 003/10" />
                                <field name="name" colspan="2" invisible="context.get('line_type', False)" placeholder="e.g. Invoice SAJ/0042" />
                                <field name="company_id" widget="selection" groups="base.group_multi_company" />
                            </group>
                            <group col="2" attrs="{'invisible':[('is_multi_currency','=',False)]}">
                                <separator string="Currency Options" colspan="2" />
                                <field name="is_multi_currency" invisible="1" />
                                <label for="payment_rate" colspan="1" />
                                <div>
                                    <field name="payment_rate" required="1" class="oe_inline" />
                                    <field name="payment_rate_currency_id" class="oe_inline" groups="base.group_multi_currency" />
                                </div>
                                <!-- <field name="currency_help_label" colspan="2" nolabel="1" class="oe_grey"/> -->
                                <field name="paid_amount_in_company_currency" colspan="2" invisible="1" />
                            </group>
                            <group col="2">
                                <separator string="Payment Options" colspan="2" />
                                <field name="writeoff_amount" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                <field name="payment_option" required="1" attrs="{'invisible':[('writeoff_amount','=',0)], 'readonly': [('state', 'not in', ('draft'))]}" />
                                <field name="writeoff_acc_id" attrs="{'invisible':['|', ('payment_option','!=','with_writeoff'), ('writeoff_amount','=',0)], 'required':[('payment_option','=','with_writeoff')], 'readonly': [('state', 'not in', ('draft'))]}" domain="[('type','=','other')]" />
                                <field name="comment" attrs="{'invisible':['|', ('payment_option','!=','with_writeoff'), ('writeoff_amount','=',0)], 'readonly': [('state', 'not in', ('draft'))]}" />
                                <field name="analytic_id" groups="analytic.group_analytic_accounting" attrs="{'invisible':['|', ('payment_option','!=','with_writeoff'), ('writeoff_amount','=',0)], 'readonly': [('state', 'not in', ('draft'))]}" />
                            </group>
                        </group>
                        <notebook>
                            <page string="Payment Information" name="payment_information">
                                <label for="payment_mode_line_ids" />
                                <field name="payment_mode_line_ids">
                                    <tree string="Payment Methods" editable="bottom" colors="gray:amount==0">
                                        <field name="payment_mode_id" invisible="0" attrs="{'required': True}" domain="[('type', 'in', ['cash', 'bank']), ('company_id', '=', parent.company_id)]" />
                                        <field name="currency_id" readonly="1" />
                                        <field name="date" />
                                        <field name="payment_order_id" invisible="1" />
                                        <field name="name" invisible="0" />
                                        <field name="amount" sum="Total" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                    </tree>
                                </field>
                                <field name="invoice_ids" widget="many2many_tags" invisible="1" />
                                <div class="oe_button_box" name="button_box">
                                    <button name="gather_debt_lines" attrs="{'invisible': [('state', 'in', ['cancel', 'posted'])]}" type="object" class="oe_stat_button" icon="fa-refresh slow-spin">
                                        Gather Lines
                                    </button>
                                </div>
                                <label for="debt_line_ids" string="Supplier Invoices and Outstanding transactions" />
                                <field name="debt_line_ids" context="{'journal_id':journal_id,
                                    'type':type, 'partner_id':partner_id, 'account_id':account_id}">
                                    <tree string="Supplier Invoices and Outstanding transactions" editable="bottom" decoration-muted="amount == 0" decoration-success="amount == amount_unreconciled">
                                        <field name="move_invoice_id" invisible="1" />
                                        <field name="state" invisible="1" />
                                        <field name="invoice_id" force_save="True" domain="[('state', '=', 'open'),
                                            ('commercial_partner_id', '=', parent.partner_id),
                                            ('id', 'not in', parent.invoice_ids),
                                            ('type', '=', 'in_invoice')]" attrs="{
                                            'required': ['|',
                                            ('move_line_id', '=', False),
                                            ('move_invoice_id', '!=', False)],
                                            'readonly': [('move_line_id', '!=', False)]}" options="{'no_quick_create': True, 'no_create_edit': True}" />
                                        <field name="ref" readonly="1" force_save="True" />
                                        <field name="move_line_id" context="{'journal_id':parent.journal_id, 'partner_id':parent.partner_id}" domain="[('account_id.internal_type','=','payable'), ('reconciled','=', False), ('partner_id','=',parent.partner_id)]" attrs="{'readonly': True}" force_save="True" />
                                        <field name="account_id" groups="base.group_no_one" domain="[('type','=','payable')]" readonly="1" force_save="True" />
                                        <field name="date_original" readonly="1" />
                                        <field name="date_due" readonly="1" />
                                        <field name="currency_id" invisible="1" />
                                        <field name="amount_original" readonly="1" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                        <field name="amount_unreconciled" readonly="1" groups="account.group_account_user" sum="Total Unreconciled" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                        <field name="reconcile" groups="account.group_account_user" invisible="1" />
                                        <field name="amount" sum="Total Allocation" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                        <field string="Full" name="reconcile" class="reconcile_bullseye" />
                                    </tree>
                                </field>
                                <div class="oe_button_box" name="button_box">
                                    <button name="gather_income_lines" attrs="{'invisible': [('state', 'in', ['cancel', 'posted'])]}" type="object" class="oe_stat_button" icon="fa-refresh slow-spin">
                                        Gather Lines
                                    </button>
                                </div>
                                <label for="income_line_ids" string="Credits" />
                                <field name="income_line_ids" context="{'journal_id':journal_id, 'partner_id':partner_id}">
                                    <tree string="Credits" editable="bottom" decoration-muted="amount == 0" decoration-success="amount == amount_unreconciled">
                                        <field name="move_invoice_id" invisible="1" />
                                        <field name="state" invisible="1" />
                                        <field name="invoice_id" force_save="True" attr="{'readonly': [('move_line_id', '!=', False)]}" domain="[('state', '=', 'open'),
                                            ('commercial_partner_id', '=', parent.partner_id),
                                            ('id', 'not in', parent.invoice_ids),
                                            ('type', '=', 'in_refund')]" attrs="{
                                            'required': ['|',
                                            ('move_line_id', '=', False),
                                            ('move_invoice_id', '!=', False)],
                                            'readonly': [('move_line_id', '!=', False)]}" options="{'no_quick_create': True, 'no_create_edit': True}" />
                                        <field name="ref" readonly="1" force_save="True" />
                                        <field name="move_line_id" context="{'journal_id':parent.journal_id, 'partner_id':parent.partner_id}" domain="[('account_id.internal_type','=','payable'), ('reconciled','=', False), ('partner_id','=',parent.partner_id)]" readonly="1" force_save="True" />
                                        <field name="account_id" groups="base.group_no_one" domain="[('type','=','payable')]" readonly="1" force_save="True" />
                                        <field name="date_original" readonly="1" />
                                        <field name="date_due" readonly="1" />
                                        <field name="currency_id" invisible="1" />
                                        <field name="amount_original" readonly="1" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                        <field name="amount_unreconciled" readonly="1" groups="account.group_account_user" sum="Total Unreconciled" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                        <field name="reconcile" groups="account.group_account_user" invisible="1" />
                                        <field name="amount" sum="Total Allocation" widget="monetary" options="{'currency_field': 'currency_id'}" />
                                        <field string="Full" name="reconcile" class="reconcile_bullseye" />
                                    </tree>
                                </field>
                                <group>
                                    <group string="Internal Notes">
                                        <field name="narration" colspan="2" nolabel="1" />
                                    </group>
                                </group>
                            </page>
                            <page string="Journal Items" name="journal_items" attrs="{'invisible': [('state','!=','posted')]}">
                                <group col="4">
                                    <!-- <field name="period_id"/> -->
                                    <!-- <field name="audit"/> -->
                                    <!-- <field name="number"/> -->
                                    <field name="move_id" />
                                </group>
                                <field name="move_line_ids" readonly="1">
                                    <tree string="Journal Items">
                                        <field name="move_id" />
                                        <field name="ref" />
                                        <field name="date" />
                                        <field name="statement_id" />
                                        <field name="partner_id" />
                                        <field name="account_id" />
                                        <field name="name" />
                                        <field name="debit" />
                                        <field name="credit" />
                                        <!-- TODO: No existe mas el campo state -->
                                        <!-- <field name="state"/> -->
                                        <!-- TODO: No existe el modelo 'account.move.reconcile' -->
                                        <!-- <field name="reconcile_id"/> -->
                                        <field name="amount_currency" groups="base.group_multi_currency" />
                                        <field name="currency_id" groups="base.group_multi_currency" />
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter" invisible="context.get('line_type', False)">
                        <field name="message_follower_ids" widget="mail_followers" />
                        <field name="message_ids" widget="mail_thread" />
                    </div>
                </form>
            </field>
        </record>

        <record id="account_payment_order_action_vendor" model="ir.actions.act_window">
            <field name="name">Supplier Payments</field>
            <field name="res_model">account.payment.order</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[('journal_id.type', '=', 'payment'), ('type','=','payment')]</field>
            <field name="context">{'type':'payment'}</field>
            <field name="view_id" eval="False" />
            <field name="search_view_id" ref="account_payment_order_view_filter_vendor_pay" />
            <field name="target">current</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    Click to create a new supplier payment.
                    </p>
                <p>
                    Odoo helps you easily track payments you make and the remaining balances you need to pay your suppliers.
                </p>
            </field>
        </record>

        <record id="account_payment_order_action_vendor_tree" model="ir.actions.act_window.view">
            <field eval="1" name="sequence" />
            <field name="view_mode">tree</field>
            <field name="act_window_id" ref="account_payment_order_action_vendor" />
        </record>

        <record id="account_payment_order_action_vendor_form" model="ir.actions.act_window.view">
            <field eval="2" name="sequence" />
            <field name="view_mode">form</field>
            <field name="view_id" ref="account_payment_order_view_form_vendor" />
            <field name="act_window_id" ref="account_payment_order_action_vendor" />
        </record>

        <menuitem id="account_payment_order_menu_vendor" action="account_payment_order_action_vendor" parent="account.menu_finance_payables" sequence="10" />
    </data>
</odoo>
